procedure changement_allie
//BUT gérer le changement de pokémons
//ENTREE le numéro du pokémon voulu
//SORTIE le pokémon voulu devient le pokémon jouable


var touche_changement, pv_tempo, pv_max_tempo, xp_tempo, xp_max_tempo, lvl_tempo, atq_tempo: ENTIER
	nom_tempo, type_tempo                    : CHAINE
	change                                   : BOOLEEN

DEBUT
	change <-- faux
	TANTQUE change = faux FAIRE
		LIRE touche_changement
		CAS touche_changement PARMI

				2:	SI pv_allie2 > 0 ALORS
						pv_tempo <--pv_allie
						nom_tempo <-- nom_allie
						pv_max_tempo <-- pv_allie_max
						type_tempo <-- type_allie
						xp_tempo <-- xp_allie
						xp_max_tempo <-- xp_max_allie
						lvl_tempo <-- lvl_allie
						atq_tempo <-- atq_allie

						pv_allie <--pv_allie2
						nom_allie <-- nom_allie2
						pv_allie_max <-- pv_allie2_max
						type_allie <-- type_allie2
						xp_allie <-- xp_allie2
						xp_max_allie <-- xp_max_allie2
						lvl_allie <-- lvl_allie2
						atq_allie <-- atq_allie2

						pv_allie2 <--pv_tempo
						nom_allie2 <-- nom_tempo
						pv_allie2_max <-- pv_max_tempo
						type_allie2 <-- type_tempo
						xp_allie2 <-- xp_tempo
						xp_max_allie2 <-- xp_max__tempo
						lvl_allie2 <-- lvl_tempo
						atq_allie2 <-- atq_tempo

						change <-- vrai

					SINON
						SI nom_allie2 <> "" ALORS
							ECRIRE nom_allie2 + " est KO"
							ECRIRE "Veuillez choisir un autre pokémon"
						SINON
							ECRIRE "Vous n'avez pas de pokemon a cet emplacement"
							ECRIRE "Veuillez choisir un autre pokémon"
						FINSI
					FINSI
				3:	SI pv_allie3 > 0
						pv_tempo <--pv_allie
						nom_tempo <-- nom_allie
						pv_max_tempo <-- pv_allie_max
						type_tempo <-- type_allie
						xp_tempo <-- xp_allie
						xp_max_tempo <-- xp_max_allie
						lvl_tempo <-- lvl_allie
						atq_tempo <-- atq_allie

						pv_allie <--pv_allie3
						nom_allie <-- nom_allie3
						pv_allie_max <-- pv_allie3_max
						type_allie <-- type_allie3
						xp_allie <-- xp_allie3
						xp_max_allie <-- xp_max_allie3
						lvl_allie <-- lvl_allie3
						atq_allie <-- atq_allie3

						pv_allie3 <-- pv_tempo
						nom_allie3 <-- nom_tempo
						pv_allie3_max <-- pv_max_tempo
						type_allie3 <-- type_tempo
						xp_allie3 <-- xp_tempo
						xp_max_allie3 <-- xp_max_tempo
						lvl_allie3 <-- lvl_tempo
						atq_allie3 <-- atq_tempo


						change <-- vrai
					SINON
						SI nom_allie3 <> "" ALORS
							ECRIRE nom_allie3 + " est KO"
							ECRIRE "Veuillez choisir un autre pokémon"
						SINON
							ECRIRE "Vous n'avez pas de pokemon a cet emplacement"
							ECRIRE "Veuillez choisir un autre pokémon"
						FINSI
					FINSI
				CASPARDEFAUT :
					ECRIRE "Veuillez entrer un nombre valide..."
		FINCASPARMI
	FINTANTQUE
FIN

procedure lvl_up
//BUT affecter les statistiques du pokemon principal en fonction de son niveau
//ENTREE rien
//SORTIE rien

DEBUT
	CAS nom_allie PARMI
		CAS "Salameche"	:	pv_allie_max <-- 36 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 1 + (3 * lvl_allie)

		CAS "Carapuce"	:	pv_allie_max <-- 44 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 1 + (3 * lvl_allie)

		CAS "Bulbizarre":	pv_allie_max <-- 50 + (3 * lvl_allie)
							pv_allie <-- pv_allie + 3
							atq_allie <-- 1 + (2 * lvl_allie)

		CAS "Roucool"	:	pv_allie_max <-- 30 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 2 + (2 * lvl_allie)

		CAS "Pikachu"	:	pv_allie_max <-- 35 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 1 + (2 * lvl_allie)

		CAS "Racaillou"	:	pv_allie_max <-- 45 + (3 * lvl_allie)
							pv_allie <-- pv_allie + 3
							atq_allie <-- 1 + (2 * lvl_allie)

		CAS "Machoc"	:	pv_allie_max <-- 39 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 2 + (3 * lvl_allie)

		CAS "Ratata"	:	pv_allie_max <-- 32 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 1 + (4 * lvl_allie)

		CAS "Magicarpe"	:	pv_allie_max <-- 36 + (2 * lvl_allie)
							pv_allie <-- pv_allie + 2
							atq_allie <-- 1 + (3 * lvl_allie)
FIN


Procedure ennemi_stats

//BUT affecter les statistiques du pokemon ennemi en fonction de son niveau
//ENTREE rien
//SORTIE rien

DEBUT
	CAS nom_ennemi PARMI
		CAS "Salameche"	:	pv_ennemi_max <-- 36 + (2 * lvl_ennemi)
							atq_ennemi <-- 1 + (3 * lvl_allie)
							type_ennemi <-- "feu"

		CAS "Carapuce"	:	pv_ennemmi_max <-- 44 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (3 * lvl_ennemmi)
							type_ennemi <-- "eau"

		CAS "Bulbizarre":	pv_ennemmi_max <-- 50 + (3 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (2 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Roucool"	:	pv_ennemmi_max <-- 30 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 2 + (2 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Pikachu"	:	pv_ennemmi_max <-- 35 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (2 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Racaillou"	:	pv_ennemmi_max <-- 45 + (3 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (2 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Machoc"	:	pv_ennemmi_max <-- 39 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 2 + (3 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Ratata"	:	pv_ennemmi_max <-- 32 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (4 * lvl_ennemmi)
							type_ennemi <-- "feu"

		CAS "Magicarpe"	:	pv_ennemmi_max <-- 36 + (2 * lvl_ennemmi)
							atq_ennemmi <-- 1 + (3 * lvl_ennemmi)
							type_ennemi <-- "feu"
	FINCASPARMI
	pv_ennemi <-- pv_ennemi_max
FIN

procédure attaque_salameche (nom : CHAINE pv_ennemi : ENTIER)
//BUT afficher et executer les attaques de Salameche
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT

								ECRIRE "1: griffe"
								ECRIRE "2: flammèche"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (3 * atq_allie)
											ECRIRE nom_allie + " Utilise Griffe"
									CAS 2: 	ECRIRE nom_allie + " Utilise Flammèche"
											SI type_ennemi = "eau" ALORS
												pv_ennemi <- pv_ennemi - (2 * atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI type_ennemi = "plante" ALORS
													pv_ennemi <- pv_ennemi - (8 * atq_allie)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (4 * atq_allie)
												FINSI
											FINSI
								FINCASPARMI
FIN


procédure attaque_carapuce
//BUT afficher et executer les attaques de Carapuce
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: Charge"
								ECRIRE "2: Pistolet a O"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (3 * atq_allie)
											ECRIRE nom_allie + " Utilise Charge"
									CAS 2: 	ECRIRE nom_allie + " Utilise Pistolet a O"
											SI (type_ennemi = "plante") OU (type_ennemi = "electrique") ALORS
												pv_ennemi <- pv_ennemi -  (1 * atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "feu") OU (type_ennemi = "roche") ALORS
													pv_ennemi <- pv_ennemi (atq_allie * 6)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (atq_allie * 3)
												FINSI
											FINSI
								FINCASPARMI
FIN

procédure attaque_bulbizarre
//BUT afficher et executer les attaques de Bulbizarre
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: Charge"
								ECRIRE "2: Fouet lianes"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (2 * atq_allie)
											ECRIRE nom_allie + " Utilise "Charge
									CAS 2: 	ECRIRE nom_allie + " Utilise Fouet lianes"
											SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
												pv_ennemi <- pv_ennemi - (atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "eau") OU (type_ennemi = "roche") ALORS
													pv_ennemi <- pv_ennemi (6 * atq_allie)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (3 * atq_allie)
												FINSI
											FINSI
								FINCASPARMI
FIN




procédure attaque_roucool
//BUT afficher et executer les attaques de Roucool
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: Vive-attaque"
								ECRIRE "2: Tornade"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (2 * atq_allie)
											ECRIRE nom_allie + " Utilise Vive-attaque"
									CAS 2: 	ECRIRE nom_allie + " Utilise Tornade"
											SI (type_ennemi = "roche") OU (type_ennemi = "electrique") ALORS
												pv_ennemi <- pv_ennemi - (1 * atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "plante") OU (type_ennemi = "combat") ALORS
													pv_ennemi <- pv_ennemi - (4 * atq_allie)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (2 * atq_allie)
												FINSI
											FINSI
								FINCASPARMI
FIN


