FIN

procedure affiche_histoire2_1
//BUT afficher l'illustration de l'histoire 2
//ENTREE rien
//SORTIE l illustration de la seconde phase d' histoire
DEBUT
	ECRIRE "	        `'::.                "
	ECRIRE "    _________H ,%%&%,            "
	ECRIRE "   /\     _   \%&&%%&%           "
	ECRIRE "  /  \___/^\___\%&%%&&           "
	ECRIRE "  |  | []   [] |%\Y&%'           "
	ECRIRE "  |  |   .-.   | ||              "
	ECRIRE "~~@._|@@_|||_@@|~||~~~~~~~~~~~~~ "
	ECRIRE "     `""") )"""`                 "
FIN

procedure affiche_histoire2_2
//BUT afficher l'illustration de l'histoire 2
//ENTREE rien
//SORTIE l illustration de la seconde phase d' histoire
DEBUT
	ECRIRE "	      .------.    "
	ECRIRE "         /        \   "
	ECRIRE "       .' /  \     `. "
	ECRIRE "_______|____________| "
FIN

