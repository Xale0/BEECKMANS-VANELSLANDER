	ECRIRE "    / ^^  ^ ^ ^\  /______________________________\ ^ ^ \ /|o|||\     "
	ECRIRE "   /  ^  ^^ ^ ^  /________________________________\  ^  /|||||o|\    "
	ECRIRE "  /^ ^  ^ ^^  ^    ||___|___||||||||||||___|__|||      /||o||||||\   "
	ECRIRE " / ^   ^   ^    ^  ||___|___||||||||||||___|__|||          | |       " 
	ECRIRE "/ ^ ^ ^  ^  ^  ^   ||||||||||||||||||||||||||||||oooooooooo| |ooooooo"
	ECRIRE "ooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo"

FIN

