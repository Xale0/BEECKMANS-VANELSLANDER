procédure attaque_machoc
//BUT afficher et executer les attaques de Machoc
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: griffe"
								ECRIRE "2: flammèche"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (2 * atq_allie)
											ECRIRE nom_allie + " Utilise Griffe"
									CAS 2: 	ECRIRE nom_allie + " Utilise Flammèche"
											SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
												pv_ennemi <- pv_ennemi - (2 * atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
													pv_ennemi <- pv_ennemi - (5 * atq_allie)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (3 * atq_allie)
												FINSI
											FINSI
								FINCASPARMI
FIN
procédure attaque_ratata
//BUT afficher et executer les attaques de Ratata
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: griffe"
								ECRIRE "2: flammèche"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (4 * atq_allie)
											ECRIRE nom_allie + " Utilise Griffe"
									CAS 2: 	ECRIRE nom_allie + " Utilise Flammèche"
											SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
												pv_ennemi <- pv_ennemi - (1 * atq_allie)
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
													pv_ennemi <- pv_ennemi - (5 * atq_allie)
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - (4 * atq_allie)
												FINSI
											FINSI
								FINCASPARMI
FIN

procédure attaque_magicarpe
//BUT afficher et executer les attaques de Magicarpe
//ENTREE 1 ou 2 en fonction de l'attaque selectionnee
//SORTIE le nom de l'attaque choisie et son effet sur l ennemi
var touche_attaque : ENTIER
DEBUT
								ECRIRE "1: griffe"
								ECRIRE "2: flammèche"
								LIRE touche_attaque
								CAS touche_attaque PARMI

									CAS 1: pv_ennemi <- pv_ennemi - (atq_allie)
											ECRIRE nom_allie + " Utilise Griffe"
									CAS 2: 	ECRIRE nom_allie + " Utilise Flammèche"
											SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
												pv_ennemi <- pv_ennemi - atq_allie
												ECRIRE "Ce n'est pas très efficace"
											SINON
												SI (type_ennemi = "feu") OU (type_ennemi = "vol") ALORS
													pv_ennemi <- pv_ennemi - atq_allie
													ECRIRE "C'est tres efficace !"
												SINON
													pv-ennemi <- pv_ennemi - atq_allie
												FINSI
											FINSI
								FINCASPARMI
FIN

procedure interace_combat
//BUT Afficher l interface du jeu
//ENTREE rien
//SORTIE l interface de combat

DEBUT
	ECRIRE "Un " + nom_Ennemi + " apparait !"
	ECRIRE nom_Ennemi
	ECRIRE "PV : " + pv_ennemi + "/" + pv_ennemi_max
	procedure affiche_ennemi


	ECRIRE nom_allie
	ECRIRE "PV : " + pv_allie + "/" + pv_allie_max
	procedure affiche_allie
FIN

procedure attaque_pascal
//BUT Réalisez le comportement du boss
//ENTREE rien
//SORTIE rien

VAR : 	type_boss : ENTIER
		

DEBUT
	//pv_boss <- 250
	//ATK <- 14
	//feu <- 1
	//eau <- 2
	//plante <- 3
	//roche <- 4
	//air <- 5
	//combat <- 6
	//electrique <- 7
	//normal <- 8

	type_boss <-- random (8)

	CAS type_boss PARMIS

		CAS 1 :		type_ennemi <-- "feu"
					SI type_ennemi = plante ALORS
							pv_ennemi <- pv_ennemi - 50
							ECRIRE "C est très efficace !"
						SINON
							SI type_ennemi = eau OU type_ennemi = roche ALORS
								pv_ennemi <- pv_ennemi - 10
								ECRIRE "Ceci n est pas tres efficace"
							SINON
								pv_ennemi <- pv_ennemi - 20
							FINSI
					FINSI
		CAS 2:		type_ennemi <-- "eau"
					SI type_ennemi = feu OU type_ennemi = roche OU type_ennemi = electrique ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI type_ennemi = plante ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 3:		type_ennemi <-- "plante"
					SI type_ennemi = eau OU type_ennemi = roche ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI type_ennemi = feu OU type_ennemi = air ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 4:		type_ennemi <-- "roche"
					SI type_ennemi = feu OU type_ennemi = "vol" ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI type_ennemi = combat ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 5:		type_ennemi <-- "vol"
					SI type_ennemi = plante OU type_ennemi = combat ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI touche_attaque = roche OU touche_attaque = electrique ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 6:		type_ennemi <-- "combat"
					SI type_ennemi = normal OU type_ennemi = roche ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI type_ennemi = vol OU type_ennemi = plante ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 7:		type_ennemi <-- "electrique"
					SI type_ennemi = eau OU type_ennemi = air ALORS
						pv_ennemi <- pv_ennemi - 50
						ECRIRE "C est très efficace !"
					SINON
						SI type_ennemi = roche OU type_ennemi = plante ALORS
							pv_ennemi <- pv_ennemi - 10
							ECRIRE "Ceci n est pas tres efficace"
						SINON
							pv_ennemi <- pv_ennemi - 20
						FINSI
					FINSI
		CAS 8:		type_ennemi <-- "normal"
					SI type_ennemi = roche ALORS
						pv_ennemi <- pv_ennemi - 10
						ECRIRE "Ceci n est pas tres efficace"
					SINON
						pv_ennemi <- pv_ennemi - 20
					FINSI
	FINCASPARMIS
	ECRIRE nom_ennemi + " est maintenant de type " + type_ennemi
	ECRIRE nom_ennemi + " utilise erreur de compilation !"
FIN


procedure tour_ennemi
//BUT determine l'action de l'ennemi
//ENTREE rien
//SORTIE l'action réalisée par l'ennemi

var attaque_ennemi : integer

DEBUT
	attaque_ennemi <- random (10)
	CAS nom_ennemi PARMI

		CAS "Salameche" 	:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (3 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Griffe"
								SINON
									ECRIRE nom_ennemi + " Utilise Flammèche"
									SI type_allie = "eau" ALORS
										pv_allie <-- pv_allie - (2 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "plante" ALORS
											pv_allie <-- pv_allie - (8 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (4 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Carapuce" 		:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (3 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Charge"
								SINON
									ECRIRE nom_ennemi + " Utilise Pistolet a O"
									SI type_allie = "plante" OU type_allie = "electrique" ALORS
										pv_allie <-- pv_allie - (1 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "feu" OU type_allie = "roche" ALORS
											pv_allie <-- pv_allie - (6 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (3 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Bulbizarre" 	:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (2 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Charge"
								SINON
									ECRIRE nom_ennemi + " Utilise Fouet lianes"
									SI type_allie = "feu" OU type_allie = "vol" ALORS
										pv_allie <-- pv_allie - (1 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "roche" OU type_allie = "eau" ALORS
											pv_allie <-- pv_allie - (6 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (3 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Roucool" 		:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (2 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Vive-attaque"
								SINON
									ECRIRE nom_ennemi + " Utilise Tornade"
									SI type_allie = "roche" OU type_allie = "electrique" ALORS
										pv_allie <-- pv_allie - (1 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "plante" OU type_allie = "combat" ALORS
											pv_allie <-- pv_allie - (4 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (2 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Pikachu" 		:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (2 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Charge"
								SINON
									ECRIRE nom_ennemi + " Utilise Eclair"
									SI type_allie = "roche" OU type_allie = "plante" ALORS
										pv_allie <-- pv_allie - (2 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "vol" OU type_allie = "eau" ALORS
											pv_allie <-- pv_allie - (6 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (4 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Racaillou" 	:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (2 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Roulade"
								SINON
									ECRIRE nom_ennemi + " Utilise Jet d pierre"
									SI type_allie = "eau" OU type_allie = "combat" ALORS
										pv_allie <-- pv_allie - (1 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "feu" OU type_allie = "vol" ALORS
											pv_allie <-- pv_allie - (4 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (2 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Machoc" 		:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (2 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Damocles"
								SINON
									ECRIRE nom_ennemi + " Utilise Balayage"
									SI type_allie = "vol" OU type_allie = "plante" ALORS
										pv_allie <-- pv_allie - (2 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										SI type_allie = "normal" OU type_allie = "roche" ALORS
											pv_allie <-- pv_allie - (5 * atq_ennemi)
											ECRIRE "C est super efficace"
										SINON
											pv_allie <-- pv_allie - (3 * atq_ennemi)
										FINSI
									FINSI
									
								FINSI

		CAS "Ratata" 		:	SI attaque_ennemi > 5 ALORS
									pv_allie <-- pv_allie - (3 * atq_ennemi)
									ECRIRE nom_ennemi + " Utilise Charge"
								SINON
									ECRIRE nom_ennemi + " Utilise Croc fatal"
									SI type_allie = "roche" ALORS
										pv_allie <-- pv_allie - (1 * atq_ennemi)
										ECRIRE "Ce n est pas tres efficace"
									SINON
										pv_allie <-- pv_allie - (4 * atq_ennemi)
									FINSI
									
								FINSI

		CAS "Magicarpe" 	:	ECRIRE nom_ennemi + " Utilise Trempette"
								ECRIRE "Mais rien ne se passe..."

		CAS "PASCAL" 		:	procedure attaque_pascal
	FINCASPARMI
FIN





procedure combat
//BUT permet le bon deoulement d'un combat
//ENTREE les actions selectionnees
//SORTIE le vainqueur du combat
DEBUT


	 //********************************************************************************************************************************************************************
	 // COMBAT
	 //********************************************************************************************************************************************************************
	victoire <-- faux
	TANTQUE victoire = faux FAIRE
		ECRIRE"Un" + nom_ennemi + " sauvage apparait!"
		TANTQUE pv_allie > 0 ET pv_ennemi > 0 et fuite = faux FAIRE
			
			procedure interface_combat

			ECRIRE "1 : attaquer"
			ECRIRE "2 : objet"
			ECRIRE "3 : fuir"
			ECRIRE "4 : changer "
			LIRE touche

		
			CAS touche PARMI
				CAS 1:  
						CAS nom_allie PARMI
							CAS "Salameche":
									procedure attaque_salameche
							CAS "Carapuce" :
									procedure attaque_carapuce
							CAS "Bulbizarre" :
									procedure attaque_bulbizarre
							CAS "Roucool" :
									procedure attaque_roucool
							CAS "Pikachu" :
									procedure attaque_pikachu
							CAS "Racaillou" :
									procedure attaque_racaillou
							CAS "Machoc" :
									procedure attaque_machoc
							CAS "Ratata" :
									procedure attaque_ratata
							CAS "Magicarpe" :
									procedure attaque_magicarpe
						FINCASPARMI
				CAS 2:
					ECRIRE "1: potion (x" + nbr_potion +")"
					ECRIRE "2: pokeballs (x" + nbr_pokeball + ")"
					LIRE touche_objet
					CAS touche_objet PARMI
						CAS 1:	SI nbr_potion > 0 ET pv_allie < pv_allie_max - 30 ALORS
									nbr_potion <- nbr_potion -1	
									ECRIRE nom_joueur + " utilise une potion"
									ECRIRE nom_allie + " a recupere 30PV"
									pv_allie <-- pv_allie + 30
								SINON
									SI nbr_potion > 0 ET pv_allie < pv_allie_max ALORS
										nbr_potion <-- nbr_potion-1
										ECRIRE nom_joueur + " Utilise une potion"
										ECRIRE "Les PV de " + nom_allie + " sont au maximum"
										pv_allie <-- pv_allie_max
									SINON
										SI nbr_potion <= 0 ALORS
											ECRIRE "Vous n'avez plus de potions"
										SINON
											ECRIRE "Vous avez trop de points de vie"
										FINSI
									FINSI
								FINSI
						CAS 2: 	chance_capture <-- random(10)
								SI nbr_pokeball > 0 ET chance_capture > 2 ET nom_allie2 = "" ALORS
									nbr_pokeball <-- nbr_pokeball - 1
									ECRIRE nom_joueur + "Utilise une pokeball"
									ECRIRE "Pokémon capturé"
									nom_allie2 <-- nom_ennemi
									pv_allie2 <-- pv_ennemi
									pv_allie2_max <-- pv_ennemi_max
									type_allie2 <-- type_ennemi
									pv_ennemi <-- 0
								SINON
									SI nbr_pokeball > 0 ET chance_capture > 2 ET nom_allie3 = "" ALORS
										nbr_pokeball <-- nbr_pokeball - 1
										ECRIRE nom_joueur + "Utilise une pokeball"
										ECRIRE "Pokémon capturé"
										nom_allie3 <-- nom_ennemi
										pv_allie3<-- pv_ennemi
										pv_allie3_max <-- pv_ennemi_max
										type_allie3 <-- type_ennemi
										pv_ennemi <-- 0
									SINON
										SI nbr_pokeball > 0 ET chance_capture > 2 ALORS
											ECRIRE"Vous n avez plus de place dans votre equipe"
											nbr_pokeball <-- nbr_pokeball - 1
										SINON
											SI chance_capture <= 2 ET nbr_pokeball > 0 ALORS
												nbr_pokeball <-- nbr_pokeball - 1
												ECRIRE "Raté"
											SINON
												ECRIRE "Vous n'avez plus de pokeballs"
											FINSI
										FINSI
									FINSI
								FINSI
					FINCASPARMI

				CAS 3:	chance_fuite <- random (10)
						SI chance_fuite > 4 ALORS
							ECRIRE "Vous n'arrivez pas à fuir"
						SINON
							fuite := vrai
						FINSI 
				CAS 4:  ECRIRE "2 : " + nom_allie2 + " (" + pv_allie2 + " pv)"
					   	ECRIRE "3 : " + nom_allie3 + " (" + pv_allie3 + " pv)"
					   	procedure changement_allie
				CASPARDEFAUT :
						ECRIRE "Veuillez entrer un nombre valide..."
			FINCASPARMI
			SI pv_allie > 0 ET pv_ennemi > 0 et fuite = faux ALORS
				tour_ennemi
			FINSI
			SI (pv_allie <= 0) ET ((pv_allie2 > 0) OU (pv_allie3 > 0)) ALORS
				ECRIRE nom_allie + "Est KO"
				ECRIRE "Choisissez son remplaçant"
				ECRIRE "2 : " + nom_allie2 + " (" + pv_allie2 + ")"
				ECRIRE "3 : " + nom_allie3 + " (" + pv_allie3 + ")"
				procedure changement_allie
				ECRIRE nom_allie + "En avant !"
			FINSI
		FINTANTQUE
		SI fuite = vrai ALORS
			ECRIRE "Vous avez fuit"
		SINON
			SI pv_allie > 0
				ECRIRE "Vous avez gagné !!!"
				xp_allie <-- xp_allie + 100
				TANTQUE (xp_allie > xp_allie_max) FAIRE
					xp_allie <-- xp_allie - xp_allie_max
					lvl_allie <-- lvl_allie +1
					xp_allie_max <-- 80 + (5 * lvl_allie)
					ECRIRE
					"Felicitations !! " + nom_allie + " passe au niveau " + lvl_allie
				FINTANTQUE
				procedure lvl_up
			SINON
				ECRIRE "Vous avez perdu !"
				ECRIRE "Tous vos PV ont été restaurés"
				pv_allie <-- pv_allie_max
				pv_allie2 <-- pv_allie2_max
				pv_allie3 <-- pv_allie3_max
				pv_ennemi <-- pv_ennemi_max
			FINSI
		FINSI
	FINTANTQUE

FIN

procedure intro
//BUT realiser l introduction du jeu
//ENTREE le nom entre par le joueur et un chiffre designant le pokemon choisi
//SORTIE le pokemon choisi un un acces a un combat avec celui-ci

VAR touche_choix : ENTIER

DEBUT
	procedure affiche_intro
	ECRIRE "??? : Bienvenue dans le monde merveilleux des Pokemons"

	procedure affiche_intro
	ECRIRE "??? : Tu t'appretes a vivre une aventure epique. En appuyant sur Entree tu ne pourras plus faire marche arriere."

	procedure affiche_intro
	ECRIRE "??? : Mais dis-moi, quel est ton nom ?"
	LIRE nom_joueur

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Enchante " + nom_joueur + " je suis le professeur Hatton !"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : L experience que tu vas vivre a ete developpee par deux de mes etudiants"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : ..."

	procedure affiche_intro
	ECRIRE "Pr. Hatton : (Esperons qu ils n aient pas fait n importe quoi...)"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Hum... Toujours est-il que je suis la pour te guider dans ton aventure"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Afin de pouvoir affronter d autres dresseurs, tu devras etre accompgne"
	ECRIRE "d un petit compagnon."


	procedure affiche_intro
	ECRIRE "Pr. Hatton : Je peux te proposer trois compagnons, a toi de faire le bon choix"
	ECRIRE "------------------------------------------------------------------"
	ECRIRE "1 : Bulbizarre"
	ECRIRE "	Type : plante"
	ECRIRE "	Niveau : 3"
	ECRIRE "	PV : 59"
	ECRIRE "	Attaque : 7"

	ECRIRE "------------------------------------------------------------------"
	ECRIRE "2 : Carapuce"
	ECRIRE "	Type : eau"
	ECRIRE "	Niveau : 3"
	ECRIRE "	PV : 50"
	ECRIRE "	Attaque : 10"

	ECRIRE "------------------------------------------------------------------"
	ECRIRE "3 : Salameche"
	ECRIRE "	Type : feu"
	ECRIRE "	Niveau : 3"
	ECRIRE "	PV : 42"
	ECRIRE "	Attaque : 10"

	LIRE touche_choix
	lvl_allie <-- 3
	lvl_ennemi <-- 3

	CAS touche_choix PARMI

			CAS 1 : nom_allie = "Bulbizarre"
					type_allie = "plante"
					nom_ennemi = "Carapuce"

			CAS 2 : nom_allie = "Carapuce"
					type_allie = "eau"
					nom_ennemi = "Salameche"

			CAS 3 : nom_allie = "Salameche"
					type_allie = "feu"
					nom_ennemi = "Bulbizarre"
	FINCASPARMI
	procedure lvl_up
	procedure ennemi_stats

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Hum... Ainsi tu as choisi " + nom_allie + " !"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Faisons donc un combat pour voir ce que tu as dans le ventre."

	procedure affiche_intro
	ECRIRE "*Le Pr. Hatton envoie un " + nom_ennemi + "*"

	procedure combat

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Tu t es bien debrouille, felicitations !"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : J ai rarement vu un talent comme le tien."

	procedure affiche_intro
	ECRIRE "Pr. Hatton : J ai entendu parler d un bug informatique au Bourg Palette."

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Il aurait ete cause par un Pokemon electrique."

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Peut-etre que toi et ton compagnon pourriez faire quelque chose..."

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Attends quelques instants, je vais soigner ton " + nom_allie + " apres ce combat eprouvant."

	pv_allie <-- pv_allie_max

	procedure affiche_intro
	ECRIRE "*Tous les PV ont été restaurés.*"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Voici egalemnt de l equipement pour ton aventure"

	procedure affiche_intro
	ECRIRE "*3 potions et 3 pokeballs ont ete rajoutees a votre inventaire*"
	nbr_potions <-- 3
	nbr_pokeballs <-- 3

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Bon courage " + nom_joueur + " !"

	procedure affiche_intro
	ECRIRE "Pr. Hatton : Et prends garde aux Pokemons sauvages"


FIN

procedure histoire1
//BUT realiser la premiere phase d histoire du jeu
//ENTREE rien
//SORTIE le lancement d un combat avec les pokemons de l allie et l affichage de la première histoire

DEBUT
	procedure affiche_histoire1_1
	ECRIRE "*Vous marchez sur un sentier en direction du Bourg Palette*"

	procedure affiche_histoire1_1
	ECRIRE "*Tout semble calme*"

	procedure affiche_histoire1_1
	ECRIRE "*Trop calme...*"

	procedure affiche_histoire1_1
	ECRIRE "*Quand tout a coup !*"

	procedure affiche_histoire1_2
	ECRIRE "*Un roucool agressif fait son apparition*"

	procedure affiche_histoire1_2
	ECRIRE "*Il se jette sans hesitation sur votre " + nom_allie +"*"

	nom_ennemi <-- "Roucool"
	lvl_ennemi <-- 4
	procedure ennemi_stats
	procedure combat

	procedure affiche_histoire1_1
	ECRIRE "*Une fois le combat termine, vous reprenez votre route*"
FIN

procedure histoire2
//BUT realiser la deuxieme phase d histoire du jeu
//ENTREE rien
//SORTIE le lancement d un combat avec les pokemons de l allie et l affichage de la deuxieme histoire

DEBUT
	procedure affiche_histoire2_1
	ECRIRE "*Vous arrivez au Bourg Palette*"

	procedure affiche_histoire2_1
	ECRIRE "*Des bruits d explosion provenant d une maison sur votre gauche attirent votre oeuil*"

	procedure affiche_histoire2_1
	ECRIRE "*Vous voyez un garcon s enfuir de la maison en flammes*"

	procedure affiche_histoire2_1
	ECRIRE "*Il se dirige vers vous en criant*"

	procedure affiche_histoire2_2
	ECRIRE "??? : AAAH ! A L AIDE !!! MON PIKACHU EST POSSEDE ! IL FAUT M AIDER !"

	procedure affiche_histoire2_3
	ECRIRE "*A peine eut-il le temps de finir sa phrase qu une souris jaune apparait au coin de la rue*"

	procedure affiche_histoire2_3
	ECRIRE "*Elle semble enragee*"

	procedure affiche_histoire2_3
	ECRIRE "*Appercevant votre Pokemon, le Pikachu se jette dessus*"

	procedure affiche_histoire2_3
	ECRIRE "PIKAAAAAAAA !"

	lvl_ennemi <-- 4
	nom_ennemi <-- "Pikachu"
	procedure ennemi_stats
	procedure combat

	procedure affiche_histoire2_3
	ECRIRE "*Le Pikachu semble a bout de forces et se dirige docilement vers son maitre*"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Enchante ! Je m appelle Sacha Ketchum"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Tu dois etre " + nom_joueur + "!"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Le Pr. Hatton m a prevenu de ton arrivee"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Il m a dit que tu etais doue pour le combat,"
	ECRIRE "        mais il a minimise ton talent !"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Desole pour ce contre temps..."

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Depuis que la Team Rocket s est installe dans notre village,"
	ECRIRE "        les Pokemons ont un comportement bizarre"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Ils appelent cela le projet Pascal"
	ECRIRE "        mais leurs intentions restent mysterieuses..."

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Ils ont base leur station scientiifque a cote du"
	ECRIRE "        dojo, peut-etre que tu pourrais aller voir ce qui s y trame"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Le dojo se trouve de l autre cote de la grotte au sud du village"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Je vais passer un peu de super potion sur tes Pokemons,"
	ECRIRE "        car la grotte est dangereuse."


	SI pv_allie < pv_allie_max - 50 ALORS
		pv_allie <-- pv_allie + 50
	SINON
		SI pv_allie < pv_allie_max ALORS
			pv_allie <-- pv_allie_max
		FINSI
	FINSI

	SI pv_allie2 < pv_allie2_max - 50 ALORS
		pv_allie2 <-- pv_allie2 + 50
	SINON
		SI pv_allie2 < pv_allie2_max ALORS
			pv_allie2 <-- pv_allie2_max
		FINSI
	FINSI

	procedure affiche_histoire2_2
	ECRIRE "*50 PV ont ete rendus a tous vos Pokemons*"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Je vais prevenir le Sensei de ton arrivee !!"

FIN

procedure histoire3
//BUT realiser la troisieme phase d histoire du jeu
//ENTREE rien
//SORTIE le lancement d un combat avec les pokemons de l allie et l affichage de la troisieme histoire
DEBUT
	procedure affiche_histoire3
	ECRIRE "*Vous penetrez dans une grotte extremement sombre*"

	procedure affiche_histoire3
	ECRIRE "*Apres quelques minutes de marche vous ne voyez plus rien, vous allumez donc votre lampe de poche*"

	procedure affiche_histoire3
	ECRIRE "*Son faisceau n eclaire que tres peu la galerie*"

	procedure affiche_histoire3
	ECRIRE "*Apres une petite periode de marche, vous trebuchez sur une pierre*"

	procedure affiche_histoire3
	ECRIRE "*Alors que vous etes a terre, vous remarquez que cette pierre possede des bras*"

	procedure affiche_histoire3
	ECRIRE "*Elle se retourne vers vous et s apprete a vous assener un coup*"

	procedure affiche_histoire3
	ECRIRE "*Vos equipiers s interposent pour vous defendre*"

	lvl_ennemi <-- 5
	nom_ennemi <-- "Racaillou"
	procedure ennemi_stats
	procedure combat

	procedure affiche_histoire3
	ECRIRE "*Apres ce combat, vous decouvrez que votre lampe ne fonctionne plus*"

	procedure affiche_histoire3
	ECRIRE "*Vous avancez a tatons dans la caverne*"

	procedure affiche_histoire3
	ECRIRE "*Finalement, vous appercevez la lumiere du jour au loin*"

	procedure affiche_histoire3
	ECRIRE "*Vous sortez enfin de cette grotte et remarquez ce qui semble etre un dojo a quelques centaines de merdres*"
FIN

