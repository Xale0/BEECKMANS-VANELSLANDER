	lvl_ennemi <-- 7
	procedure ennemi_stats
	procedure combat

	procedure affiche_histoire4
	ECRIRE "*Le machoc sonne semble avoir retrouve ses esprits*"

	procedure affiche_histoire4
	ECRIRE"*Le dojo, si plein de vie actuellement vous semble totalement vide*"

	procedure affiche_histoire4
	ECRIRE "*Machoc, inquiet, pointe une immense tour decoree d un grand R rouge*"
	ECRIRE "*Que vous pouvez appercevoir au loin*"

	procedure affiche_histoire4
	ECRIRE "*Votre telephone se met subitement a sonner*"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Allo " + nom_joueur + ", le sensei ne repond plus, il a probablement"
	ECRIRE "        ete capture par la Team Rocket, il faut que tu ailles l aider"

	procedure affiche_histoire2_2
	ECRIRE "Sacha : Tu pourrais sans doute fouiller le dojo pour trouver des outils qui t aideront."

	procedure affiche_histoire4
	ECRIRE "*En fouillant le dojo vous trouvez differents remedes*"

	procedure affiche_histoire4
	ECRIRE "*Tous les PV ont ete restaures*"

	pv_allie <-- pv_allie_max
	pv_allie2 <-- pv_allie2_max
	pv_allie3 <-- pv_allie3_max

	procedure affiche_histoire4
	ECRIRE "*5 potions ont ete rajoutees a votre inventaire*"
	nbr_potion <-- nbr potion + 5

	procedure affiche_histoire4
	ECRIRE "*3 pokeballs ont ete rajoutees a votre inventaire*"
	nbr_pokeball <-- nbr_pokeball + 3
FIN

