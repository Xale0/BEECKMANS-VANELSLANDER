ALGORITHME : combat



//BUT : réaliser un combat entre deux pokémons
//ENTREE : des entiers représentants des actions
//SORTIE : lequel des deux pokémons l'emporte


VAR pv_allie, pv_allie_max, lvl_allie, xp_allie, xp_allie_max, atq_allie pv_allie2, pv_allie2_max, lvl_allie2, xp_allie2, xp_allie2_max, atq_allie2, pv_allie3, pv_allie3_max, lvl_allie3, xp_allie3, xp_allie3_max, atq_allie3 pv_ennemi, pv_ennemi_max, atq_ennemi, touche, touche_objet, nbr_potion, nbr_pokeball, chance_fuite, chance_capture : ENTIER
	nom_Ennemi, nom_Allie type_allie, type_ennemi : CHAINE
	fuite, dresseur: BOOLEEN


procedure affiche_intro
//BUT afficher l'illustration de l'intro
//ENTREE rien
//SORTIE l illustration de l intro

DEBUT
	ECRIRE "   	 o     "
	ECRIRE "    /|\    "
	ECRIRE "    / \    "
FIN

procedure affiche_histoire1_1
//BUT afficher l'illustration de l'histoire 1
//ENTREE rien
//SORTIE l illustration de la première phase d' histoire
DEBUT
	ECRIRE"               ,@@@@@@@,                   "
	ECRIRE"       ,,,.   ,@@@@@@/@@,  .oo8888o.       "
	ECRIRE"    ,&%%&%&&%,@@@@@/@@@@@@,8888\88/8o      "
	ECRIRE"   ,%&\%&&%&&%,@@@\@@@/@@@88\88888/88'     "
	ECRIRE"   %&&%&%&/%&&%@@\@@/ /@@@88888\88888'     "
	ECRIRE"   %&&%/ %&%%&&@@\ V /@@' `88\8 `/88'      "
	ECRIRE"   `&%\ ` /%&'    |.|        \ '|8'        "
	ECRIRE"       |o|        | |         | |          "
	ECRIRE"       |.|        | |         | |          "
	ECRIRE"   \\/ ._\//_/__/  ,\_//__\\/.  \_//__/_   "
FIN


